#user defined 
#grep 
alias gp='grep -nr' 
#dirs operation command 
alias ..='cd ..' 
alias ...='cd ../..' 
alias ....='cd ../../..' 
alias .....='cd ../../../..' 
# git relevent 
alias gts='git status .' 
# change directory to the git root directory 
alias gtr='cd "$(git rev-parse --show-toplevel)"'
